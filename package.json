{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "concurrently -p \"[{name}]\" -n \"tailwind,remix\" -c \"blue.bold,red.bold\" \"npm:dev:css\" \"npm:dev:remix\"",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "start": "remix-serve build",
    "dev:docker": "docker compose up -d",
    "dev:docker:stop": "docker compose down",
    "lint": "npx eslint \"**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.8.4",
    "@mantine/hooks": "^5.8.4",
    "@mantine/remix": "^5.8.4",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@tabler/icons": "^1.114.0",
    "bullmq": "^3.2.3",
    "ioredis": "^5.2.4",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-eslint": "^15.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9"
  },
  "engines": {
    "node": ">=14"
  }
}
